{
  "name": "@cerebralutopia/tap",
  "version": "0.1.0",
  "description": "Process supervisor with queryable logs",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "tap": "./bin/tap"
  },
  "files": [
    "dist",
    "bin",
    "skill.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "process",
    "supervisor",
    "daemon",
    "logs"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bbatchelder/tap.git"
  },
  "author": "bbatchelder",
  "license": "MIT",
  "homepage": "https://github.com/bbatchelder/tap#readme",
  "bugs": {
    "url": "https://github.com/bbatchelder/tap/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "commander": "^12.0.0",
    "node-pty": "^1.0.0",
    "undici": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
